{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "comma-dangle": ["error", "never"],
        "import/extensions": 0,
        "import/no-unresolved": 1,
        "indent": [ "error", 4 ],
        "keyword-spacing": 0,
        "space-in-parens": [ "error", "always" ]
    }
}
